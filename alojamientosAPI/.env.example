# Database Configuration - Development Environment
# Copy this file to .env and fill in your actual credentials
# NEVER commit the .env file to git

# Development Database
DB_DEV_URL=jdbc:postgresql://localhost:5434/your_dev_database
DB_DEV_USER=your_dev_username
DB_DEV_PASSWORD=your_dev_password

# Production Database
DB_PROD_URL=jdbc:postgresql://localhost:5435/your_prod_database
DB_PROD_USER=your_prod_username
DB_PROD_PASSWORD=your_prod_password

# JWT Configuration (if needed)
JWT_SECRET=your_jwt_secret_key_here_minimum_256_bits
JWT_EXPIRATION=86400000
